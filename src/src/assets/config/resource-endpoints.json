{
  "environments": [
    {
      "name": "development",
      "endpoints": [
        {
          "resource": "AggregateRoot",
          "baseUrl": "http://localhost:4300",
          "url": "aggregate-roots",
          "versions": [
            {
              "verb": "default",
              "value": "v1"
            }
          ]
        },
        {
          "resource": "RootParent",
          "baseUrl": "http://localhost:4300",
          "url": "aggregate-roots/:id/root-parents",
          "versions": [
            {
              "verb": "default",
              "value": "v1"
            }
          ]
        },
        {
          "resource": "RootChild",
          "baseUrl": "http://localhost:4300",
          "url": "aggregate-roots/:id/root-parent/:id/root-children",
          "versions": [
            {
              "verb": "default",
              "value": "v1"
            }
          ]
        },
        {
          "resource": "RootGrandchild",
          "baseUrl": "http://localhost:4300",
          "url": "aggregate-roots/:id/root-parent/:id/root-child/:id/root-grandchildren",
          "versions": [
            {
              "verb": "default",
              "value": "v1"
            }
          ]
        }
      ]
    }
  ]
}
